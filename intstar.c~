#include <stdio.h>
#include <stdlib.h>
#include <limits.h>
#include <assert.h>

int main(){
  char *index[3];
  char tmp[2];
  int code[2] = {0,1};
  int i = 0;
  for(i = 0; i < 2; i++){
    tmp[i] = (char)code[i];
    printf("%c\n",tmp[i]);
  }
  for(i = 0; i < 3; i++){
    index[i] = tmp;
  }
  printf("%c\n",tmp[1]);
}
  
